use PHPUnit\Framework\TestCase;

/**
* 
*/
class QueueTypeTest extends TestCase
{
	public function testAddType()
	{	
		//load enviroment variables
		$dotenv = new Dotenv\Dotenv(__DIR__);
		$dotenv->load();

		//create conection
		$talker = Talker::create( $_ENV['MKT_IP'], $_ENV['MKT_USER'], $_ENV['MKT_PASS']);		
	    
	    $queuetype  = new Type($talker);
	    $result = $queuetype->addPCQ("PCQ_download","pcq","64000","dst-address");
	    $result = $queuetype->addPCQ("PCQ_upload","pcq","32000","src-address");
	    //$result = $queuetype->print();	

	    var_dump($result);

		// foreach ($listQueues as $queue) {
		// 	print_r( "\n".$queue['name'] .':'. $queue['target'] );
		// }
		
	}
	
}
